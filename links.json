---
---
[
  {% for post in site.posts %}
    { "text": "{{ post.title | replace:'"','\"' | strip_newlines }}", "href": "{{ post.url }}" },
  {% endfor %}

  {% for page in site.pages %}
     { "text": "{{ page.title | replace:'"','\"' }}", "href": "{{ page.url }}" }
     {% unless forloop.last %}, {% endunless %}
  {% endfor %}
]